.grid-view
{
        padding: 15px 0 ;
}

.grid-view table.items
{
        background: white;
        border-collapse: collapse;
        border: 1px #D0E3EF solid;
}
.list-view .pager,.grid-view .pager,.list-view .summary,.grid-view .summary{text-align:left !important;}
.group {margin:10px 0;}
.group table.detail-view th{width:280px !important;}
.group table.detail-view th, table.detail-view td {vertical-align:middle !important;}
.compactRadioGroup {display:inline-block;}
.compactRadioGroup label{font-weight:normal !important;float:left;line-height:20px;margin:0 5px;}
.compactRadioGroup input{float:left;}
.compactRadioGroup br {display:none;}

#content a.print {background: url("../images/print.gif") no-repeat scroll 0 0 transparent;margin-top: 6px;
    padding: 0 0 0 19px;}
